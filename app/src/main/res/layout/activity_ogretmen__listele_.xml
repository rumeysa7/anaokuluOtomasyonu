<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Ogretmen_Listele_Activity">

    <include layout="@layout/actionbar_app"
        android:id="@+id/actionbarlogin"/>
    <TextView
        android:id="@+id/txt_Student_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/actionbarlogin"
        android:layout_marginTop="10dp"
        android:text="Öğretmen Listesi"
        android:gravity="center"
        android:textColor="@color/colorBlack"
        android:textSize="28dp" />

    <ListView
        android:id="@+id/teacher_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/txt_Student_list"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="65dp"
        android:padding="3dp"/>


</RelativeLayout>